      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2A7FFF"
      },
      "package": "com.xerobookz.ess"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "xerobookz"
  }
}

